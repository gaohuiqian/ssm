<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="simple" targetRuntime="MyBatis3Simple">
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/pay_order"
                        userId="root"
                        password="123456"/>

        <javaModelGenerator targetPackage="com.wqd.pojo" targetProject="src/main/java"/>

        <sqlMapGenerator targetPackage="com.wqd.mapper" targetProject="src/main/resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wqd.mapper" targetProject="src/main/java"/>



        <table tableName="emp" />
    </context>
</generatorConfiguration>